<%- model_class = Individualcategory -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize%></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @individualcategories.each do |individualcategory| %>
      <tr>
        <td><%= link_to individualcategory.id, individualcategory_path(individualcategory) %></td>
        <td><%= individualcategory.name %></td>
        <td>
          <%= link_to t('.show', :default => t("helpers.links.show")),
                      individualcategory_path(individualcategory), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_individualcategory_path(individualcategory), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      individualcategory_path(individualcategory),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_individualcategory_path,
            :class => 'btn btn-primary' %>
